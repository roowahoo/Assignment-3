{{#extends 'base'}}
{{#block 'content'}}
<table class='table'>
    <thead>
        <tr>
            <th>Product Name</th>
            <th>Quantity</th>
            <th>Customer Name</th>
            <th>Shipping Address</th>
            <th>Contact</th>
            <th>Amount</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        {{#each orders}}
        <tr>
            <td>{{this.products.name}}</td>
            <td>{{this.quantity}}</td>
        </tr>
        {{/each}}
    </tbody>

</table>
{{/block}}
{{/extends}}